import random

num = random.randint(1, 100)

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True

if is_prime(num):
    print(f"{num} is prime.")
else:
    print(f"{num} is not prime.")

